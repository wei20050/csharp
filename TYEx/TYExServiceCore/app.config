                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost/TYExServiceCore/Service/" />
                    </baseAddresses>
                </host>
            </service>
        </services>
    </system.serviceModel>
