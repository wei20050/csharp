<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
      <bindings>
        <!--此处通用配置设置超时时间和数据大小限制-->
        <basicHttpBinding>
          <binding closeTimeout="00:10:00" receiveTimeout="00:20:00" sendTimeout="00:20:00"
